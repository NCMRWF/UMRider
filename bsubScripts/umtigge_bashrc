# Source global definitions
if [ -f /etc/bashrc ]; then
        . /etc/bashrc
fi

PS1="[`hostname -s`]["'$PWD]> '

export UVCDAT_ANONYMOUS_LOG=no
# load the required modules to run UMRider
module unload GNU-UVCDAT-IRIS/2.4.0_1.10.0d0
module load GNU-IRIS-TIGGE-1.10/1.10
module load GNU-qt/4.8.4
module load GNU-gribapi/1.21.0
module load GNU-wgrib2/2.0.4
module load cnvgrib/1.4.1
module load GNU-grads/grads-2.1.a2
module load cdo/1.6.4
# end of moulde load
